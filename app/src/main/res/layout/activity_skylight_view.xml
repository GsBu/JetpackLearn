<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".skylight.SkylightViewActivity">
    <com.jobs.android.jetpacklearn.view.SkylightView
        android:id="@+id/slv_my"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_margin="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:slv_orientation="vertical"
        app:slv_radius="80dp"
        app:slv_count="5"
        app:slv_divideWidth="2dp"
        app:slv_divideColor="#ffffff"
        android:background="@drawable/ic_launcher_background"/>
    <com.jobs.android.jetpacklearn.view.SkylightView
        android:id="@+id/slv_my2"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_margin="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/slv_my"
        app:slv_radius="80dp"
        app:slv_count="5"
        app:slv_divideWidth="2dp"
        app:slv_divideColor="#ffffff"
        android:background="@drawable/ic_launcher_background"/>
</androidx.constraintlayout.widget.ConstraintLayout>